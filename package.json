{
  "name": "hr-portfolio",
  "version": "2.0.0",
  "description": "Modern AI/ML Portfolio with PWA, Web Components, and Performance Optimizations",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.html,.css",
    "lint:fix": "eslint . --ext .js,.html,.css --fix",
    "format": "prettier --write .",
    "analyze": "vite-bundle-analyzer",
    "lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "keywords": [
    "portfolio",
    "pwa",
    "web-components",
    "ai-ml",
    "computer-vision",
    "modern-web",
    "performance",
    "accessibility"
  ],
  "author": "Yarrapu Reddy Hemanth Reddy",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lighthouse": "^11.4.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.17.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "workbox-window": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{html,css,scss,json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
